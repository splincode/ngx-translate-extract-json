    "description": "Extract translatable (ngx-translate) strings and save as a JSON",
    "scripts": {
        "build": "rimraf dist && npm run lint && tsc",
        "watch": "tsc -w",
        "lint": "tslint -p tsconfig.json -c tslint.json src/**/*.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Angular-RU/ngx-translate-extract-json.git"
    },
