    "homepage": "https://github.com/Angular-RU/ngx-translate-extract-json#readme",
    "main": "build/extractor.js",
    "devDependencies": {
        "tslint": "^5.9.1",
        "typescript": "~2.4.2"
    },
    "dependencies": {
        "@types/node": "^9.3.0",
        "app-root-path": "^2.0.1",
        "codelyzer": "^4.0.2",
        "command-line-args": "^5.0.0",
        "glob": "^7.1.2",
        "mkdirp": "^0.5.1",
        "path": "^0.12.7",
        "rimraf": "^2.6.2",
        "winston-color": "^1.0.0"
